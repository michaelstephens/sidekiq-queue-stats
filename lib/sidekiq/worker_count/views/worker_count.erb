<header class="row">
  <div class="col-sm-5">
    <h3>Worker Count</h3>
  </div>
</header>

<%= csrf_tag if respond_to?(:csrf_tag) %>
<% @queues.each do |q| %>
  <h4><%= q %></h4>
  <table class="table table-striped table-bordered table-white">
    <thead>
      <tr>
        <th>Name</th>
        <th>Count</th>
      </tr>
    </thead>
    <% @workers[q].each do |worker| %>
      <tr>
        <td><%=worker.first%></td>
        <td><%=worker.last%></td>
      </tr>
    <% end %>
  </table>
<% end %>
